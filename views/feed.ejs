<% include ./partials/header.ejs %>
<div class="w-full min-h-screen bg-zinc-700 text-white px-10 py-8">
  <div class="cards flex flex-wrap gap-10">
    <% posts.forEach(function(elem){ %>
    <div class="card w-32">
        <div class="w-full h-full rounded-lg overflow-hidden ">
            <img src="/images/uploads/<%= elem.Image %>" alt="" class="object-cover w-full h-full">
        </div>
        <h5 class="mt-2 text-xl"><%= elem.title %></h5>
    </div>
    <% }) %>
  </div>
</div>
<% include ./partials/footer.ejs %>